  <!-- Language -->
  <meta http-equiv="content-language" content="en" />

  <!-- Author and Publisher -->
  <meta name="author" content="Adeniji Olajide" />
  <meta name="publisher" content="Adeniji Olajide" />
  <meta name="copyright" content="Adeniji Olajide" />

  <!-- SEO Keywords -->
  <meta name="keywords"
    content="software engineer, web developer, freelance, full stack, backend development, frontend, Python, Node.js, Vue.js, MongoDB, MySQL, Redis, API design" />

  <!-- Description -->
  <meta name="description"
    content="Adeniji Olajide, Full Stack Software Engineer specializing in web development, backend development, and DevOps. Available for freelance projects. Contact me today!" />

  <!-- Page Type -->
  <meta name="page-type" content="portfolio" />
  <meta name="audience" content="Everyone" />

  <!-- Robots -->
  <meta name="robots" content="follow, index, max-snippet:-1, max-video-preview:-1, max-image-preview:large" />

  <!-- Open Graph (OG) Meta Tags for Social Sharing -->
  <meta property="og:title" content="Adeniji Olajide | Full Stack Developer Available for Freelance Work" />
  <meta property="og:description"
    content="I specialize in web development, backend engineering, and DevOps. Reach out for your next project!" />
  <meta property="og:image" content="<%= url %>portfolioimage/quantumweb.jpg" />
  <meta property="og:image:alt" content="Adeniji Olajide Portfolio Picture" />
  <meta property="og:type" content="website" />
  <meta property="og:site_name" content="Adeniji Olajide Portfolio" />
  <meta property="og:locale" content="en" />
  <meta property="og:url" content="<%= url %>" />
  <meta property="og:logo" content="./img/quantum.png" />

  <!-- Twitter Cards -->
  <meta name="twitter:card" content="summary_large_image" />
  <meta name="twitter:image" content="<%= url %>portfolioimage/quantumweb.jpg" />
  <meta name="twitter:image:alt" content="Adeniji Olajide Portfolio Picture" />
  <meta name="twitter:description"
    content="Explore the portfolio of Adeniji Olajide, a Full Stack Software Engineer and Web Developer. Available for freelance projects." />
  <meta name="twitter:site" content="@Goldenthrust3" />
  <meta name="twitter:url" content="<%= url %>" />
  <meta name="twitter:domain" content="<%= hostname %>" />

  <!-- Canonical Link -->
  <link rel="canonical" href="<%= url %>" />

  <!-- Structured Data (JSON-LD) for SEO -->
  <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@type": "Person",
        "name": "Adeniji Olajide",
        "jobTitle": "Full Stack Software Engineer",
        "url": "<%= url %>",
        "sameAs": [
          "https://github.com/GoldenThrust",
          "https://www.linkedin.com/in/olajide-adeniji/"
        ],
        "description": "Adeniji Olajide is a Full Stack Software Engineer specializing in web development, backend systems, and freelance work. Available for new projects and collaborations."
      }
    </script>
</head>

